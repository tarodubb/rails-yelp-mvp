<div class="container">
  <h2><%= @restaurant.name %><h1>
  <h3><%= @restaurant.address %></h3>
  <h3><%= @restaurant.phone_number %></h3>
  <h3><%= @restaurant.category %></h3>
</div>

<%= link_to "Create Review", new_restaurant_review_path(@restaurant)%>

<h4>Reviews</h4>
<p><% @reviews.each do |review| %>
    <%= review.rating %>
    <%= review.content %>
    <% end%>
</p>
